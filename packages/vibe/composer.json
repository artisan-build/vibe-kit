{
    "name": "artisan-build/vibe",
    "description": "A Laravel package for Vibe",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artisan Build",
            "email": "info@artisanbuild.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "pestphp/pest": "^2.0",
        "rector/rector": "^0.18.0"
    },
    "autoload": {
        "psr-4": {
            "ArtisanBuild\\Vibe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtisanBuild\\Vibe\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtisanBuild\\Vibe\\VibeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
